# Luna Agent Test Configuration
# Add cloud STT API keys for production use

# OpenAI API Key for GPT models
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key for Claude models  
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ElevenLabs API Key for Voice (Nova Westbrook)
ELEVEN_API_KEY=your_elevenlabs_api_key_here

# STT Configuration for Testing
# Leave these empty for now - system will gracefully handle missing keys
AZURE_SPEECH_KEY=
AZURE_SPEECH_REGION=eastus
DEEPGRAM_API_KEY=

# STT Engine Settings
CLOUD_STT_SERVICE=azure
STT_PREFER_LOCAL=false

# Server Configuration
PORT=3000
NODE_ENV=development

# Voice Configuration
VOICE_ENABLED=true
VOICE_LANGUAGE=en-US
STT_PROVIDER=hybrid
VOICE_AUTO_LISTEN=false
VOICE_ACTIVITY_DETECTION=true
WAKE_WORD=luna

# Debug Settings
DEBUG_STT=true
